{
    "manifest_version": 3,
    "name": "WhatsApp Message Logger",
    "version": "1.0",
    "description": "Logs WhatsApp messages",
    "permissions": ["scripting","activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://web.whatsapp.com/*"],
        "js": ["content.js"]
      }
    ]
  }